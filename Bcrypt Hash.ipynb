    "import bcrypt\n",
    "import time\n",
    "  \n",
    "# example password\n",
    "password = 'MikeJordan@12'\n",
    "\n",
    "start = time.time()  \n",
    "bytes = password.encode('utf-8')\n",
    "salt = bcrypt.gensalt()\n",
    "hash = bcrypt.hashpw(bytes, salt)\n",
    "end = time.time()\n",
    "\n",
    "print(end - start)\n",
    "  \n",
    "print(hash)"
   ]
